    <script type="text/javascript">
        var comp_jquery = $;
        comp_jquery.ajax({
            url: 'https://www.marinespecies.org/aphia.php?p=rest&__route__/AjaxGetCitationsCount?title=%22World+Register+of+Marine+Species%22&token=654e63ae90e2a994&identifier=61c6dae33d794d477e6a68ed43f52eb3',
            method: "GET",
            success: function(result){
                var valueClean = result.replace('.',' ').replace(',','');
                if (valueClean >= 0 && !isNaN(valueClean) && isFinite(valueClean)) {
                    comp_jquery('#cit_count').text(valueClean.replace(',',' '));
                } else {
                    comp_jquery('#cit_count').text('unable to update');
                }
            }
        });
    </script>

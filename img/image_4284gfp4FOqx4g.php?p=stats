<h3>Web statistics<small>&nbsp;(<b><i>including webservice usage</i></b>)</small></h3>
<div id="web_stats"></div>
<script type="text/javascript">
    var webstats_monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun",
        "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"
    ];

    function webstats_getTableTemplate(first_col)
    {
        if (typeof(first_col) === "undefined") {
            first_col = "Month";
        }
        return "<table border=\"0\" class=\"table\">\n" +
            "   <tbody>" +
            "      <tr>\n" +
            "         <th>" + first_col + "</th>\n" +
            "         <th style=\"text-align: right;\">Unique visitors</th>\n" +
            "         <th style=\"text-align: right;\">Number of visits</th>\n" +
            "         <th style=\"text-align: right;\">Pages</th>\n" +
            "         <th style=\"text-align: right;\">Hits</th>\n" +
            "         <th style=\"text-align: right;\">Bandwidth</th>\n" +
            "      </tr>" +
            "   </tbody>" +
            "</table>";
    }
    function webstats_getHtmlRow(entry, title)
    {
        if (entry[0] == "0"
            && !entry[1] == "0"
            && !entry[2] == "0"
            && !entry[3] == "0"
            && !entry[4] == "0") {
            return null;
        }
        return "<tr>\n" +
            "   <td>" + title + "</td>\n" +
            "   <td style=\"text-align: right;\">" + entry[0].toString().replace(new RegExp('(\\.|\\,)(?=(\\d{3})+)','g'),'&nbsp;') + "</td>\n" +
            "   <td style=\"text-align: right;\">" + entry[1].toString().replace(new RegExp('(\\.|\\,)(?=(\\d{3})+)','g'),'&nbsp;') + "</td>\n" +
            "   <td style=\"text-align: right;\">" + entry[2].toString().replace(new RegExp('(\\.|\\,)(?=(\\d{3})+)','g'),'&nbsp;') + "</td>\n" +
            "   <td style=\"text-align: right;\">" + entry[3].toString().replace(new RegExp('(\\.|\\,)(?=(\\d{3})+)','g'),'&nbsp;') + "</td>\n" +
            "   <td style=\"text-align: right;\">" + entry[4].toString().replace(new RegExp('(\\.|\\,)(?=(\\d{3})+)','g'),'&nbsp;') + "</td>\n" +
            "</tr>";
    }
    function updateWebStats()
    {
        comp_jquery(document).ready(function() {
            comp_jquery.ajax({
                url: 'aphia.php?ublock=0&p=stats&request_type=ajax&type=web',
                method: 'GET',
                success: function(result){
                    stats = result;

                    var year_table = comp_jquery(webstats_getTableTemplate("Year total"));
                    var year_rows = year_table.find("tbody");

                    var new_table = comp_jquery(webstats_getTableTemplate("Month"));
                    var rows = new_table.find("tbody");

                                        var year = 2023;
                    var last_month = 4;
                    var years_still_to_process = 9;
                    for (var counter = 0; counter < 12; counter++) {
                        if (!(year.toString() in stats) || !((last_month-1) in stats[year.toString()])) {
                            continue;
                        }
                        var entry_to_show = stats[year.toString()][(last_month-1)];
                        var html = webstats_getHtmlRow(entry_to_show, webstats_monthNames[last_month-1] + " " + year.toString());
                        if (html) {
                            rows.append(comp_jquery(html));
                        }
                                                last_month--;
                        if (last_month === 0) {
                                                        last_month = 12;
                            year--;
                            years_still_to_process--;
                        }
                    }

                                        while (years_still_to_process-- >= 0
                        && (year.toString() in stats)
                        && (12 in stats[year.toString()])
                        ) {
                        html = webstats_getHtmlRow(stats[year.toString()][12], year.toString());
                        if (html) {
                            year_rows.append(comp_jquery(html));
                        }
                        year--;
                    }
                                        var stats_element = comp_jquery('#web_stats');
                    stats_element.empty();
                    stats_element.append(new_table);
                    stats_element.append(year_table);

                                        comp_jquery("#update_webstats_message").hide();
                }
            });
        });
    }

        updateWebStats();
</script>                </div>
            </div>
                    </div>
    </div>
</div>
<script type="text/javascript">
    function stats_updateImageLinks() {
        aphia_jquery('.pop').each(function () {
            var thisObj = aphia_jquery(this);
            thisObj.attr('href', thisObj.find('img').attr('src') + '&resolution=fullhd');
        });
    }
    aphia_jquery(document).ready(function () {
        stats_updateImageLinks();
    });
</script>

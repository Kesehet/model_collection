                            }

                            function handleRowVisibility() {
                                                                var option_rows_to_hide = [], options_rows_to_show = [];
                                var items = aphia_jquery('#show_hide_container').find("input");
                                var length = items.length;
                                for (var index = 0; index < length; index++) {
                                    var cur_checkbox = aphia_jquery(items[index]);
                                    if (!cur_checkbox.prop('checked')) {
                                        option_rows_to_hide.push(cur_checkbox.attr("id"));
                                    } else {
                                        options_rows_to_show.push(cur_checkbox.attr("id"));
                                    }
                                }
                                var visible_count = options_rows_to_show.length;

                                                                var indexes_to_show = [];
                                var table_instance = getTableInstance();
                                                                table_instance.bootstrapTable('filterBy', {});
                                var data = table_instance.bootstrapTable('getData');
                                length = data.length;

                                                                var id_to_index_translation = [];
                                var translate_mapper = [];
                                for (index = 0; index < length; index++) {
                                                                        id_to_index_translation[data[index].id] = index;

                                                                        if (!data[index].org_name) {
                                        data[index].org_name = data[index].name;
                                        data[index].org_pid = data[index].pid;
                                    } else {
                                                                                data[index].name = data[index].org_name;
                                        data[index].pid = data[index].org_pid;
                                    }

                                                        if (visible_count === 1) {
                                                                if (data[index].org_name.includes(options_rows_to_show)) {
                                                                        indexes_to_show.push(data[index].id);
                                                                        translate_mapper[index] = data[index].pid;
                                }
                            } else if (!data[index].org_name.includes(option_rows_to_hide[0]) && !data[index].org_name.includes(option_rows_to_hide[1])) {
                                                                indexes_to_show.push(data[index].id);
                            }
                        }

                                                                var parent_id, parent_index;
                                for (var source_index in translate_mapper) {
                                                                        parent_id = translate_mapper[source_index];
                                    if (parent_id in id_to_index_translation) {
                                        parent_index = id_to_index_translation[parent_id];
                                                                                data[source_index].name = data[parent_index].org_name;

                                                                                                                        var index_of_child_that_replaced_grand_parent = translate_mapper.indexOf(data[parent_index].org_pid);
                                        if (index_of_child_that_replaced_grand_parent > -1) {
                                            data[source_index].pid = data[index_of_child_that_replaced_grand_parent].id;
                                        } else {
                                                                                        data[source_index].pid = data[parent_index].pid;
                                        }
                                    }
                                }
                                                                table_instance.bootstrapTable('filterBy', { id : indexes_to_show});
                            }

                                                        getTableInstance().bootstrapTable({
                                treeShowField: 'name',
                                data: getInitialData(),
                                icons: {
                                    columns: 'fa fa-columns'
                                }
                            });

                                                        aphia_jquery('#show_hide_container').find("input").each(function(){
                                aphia_jquery(this).on("click", function () {
                                    handleRowVisibility();
                                });
                            });

                                                        handleRowVisibility();

                            aphia_jquery(document).ready(function(){
                                                                // Link buttons to on top display opts
                                aphiaContentOptsUpdateAutoPost(false);
                                aphiaContentOptsSyncControl(
                                    'Extant',
                                    {
                                        opt_name_to_sync: 'extant',
                                        control_changed_callback : function (event) {
                                            handleRowVisibility();
                                        }
                                    }
                                );
                            });
                        </script>
                    </div>
                </div>
            </div>

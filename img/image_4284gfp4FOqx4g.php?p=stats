                        <script type="text/javascript">
                            var discovery_export = aphia_jquery("#stats_discovery_export");
                            var discovery_image = aphia_jquery("#stats_discovery_img");
                            var discovery_container = aphia_jquery("#stats_discovery");
                            var discovery_wait_container = aphia_jquery("#stats_discovery_wait");

                                                        discovery_image.on("load", function() {
                                discovery_wait_container.hide();
                                discovery_container.show();
                            });

                            function stats_discoveryOptsChanged() {
                                discovery_container.hide();
                                discovery_wait_container.show();

                                var selectedId = aphia_jquery("input[name='AphiaID_discovery']").val();
                                if (isNaN(selectedId) || !isFinite(selectedId) || selectedId <= 0) {
                                    selectedId = -1;
                                }
                                var url = "aphia.php?p=stats_graph&type=discovery";
                                url += "&id=" + selectedId;

                                var rankValue = aphia_jquery("input[name='stats_discovery_rank']:checked").val();
                                if (rankValue != 220) {
                                    url += "&rank=" + rankValue;
                                }
                                var acceptedValue = aphia_jquery("input[name='stats_discovery_accepted']:checked").val();
                                if (acceptedValue == 1) {
                                    url += "&accepted=1";
                                }

                                                                discovery_image.attr('src', url);
                                discovery_export.attr('href', url + '&export=1');
                                stats_updateImageLinks();
                            }

                                                        aphia_jquery("input[name='stats_discovery_rank']").change(function(){
                                stats_discoveryOptsChanged();
                            });
                            aphia_jquery("input[name='stats_discovery_accepted'").change(function () {
                                stats_discoveryOptsChanged();
                            });
                            aphia_jquery("#d586e69aa5ed62a8_storage").change(function() {
                                stats_discoveryOptsChanged();
                            });
                        </script>
                    </div>
